{
  "name": "99tech-code-challenge-backend",
  "version": "1.0.0",
  "description": "99Tech Code Challenge - Backend Role Solutions",
  "main": "index.js",
  "workspaces": [
    "src/problem4",
    "src/problem5",
    "src/problem6"
  ],
  "scripts": {
    "install:all": "npm install && npm run install:problem4 && npm run install:problem5",
    "install:problem4": "cd src/problem4 && npm install",
    "install:problem5": "cd src/problem5 && npm install",
    "build:all": "npm run build:problem4 && npm run build:problem5",
    "build:problem4": "cd src/problem4 && npm run build",
    "build:problem5": "cd src/problem5 && npm run build",
    "test:all": "npm run test:problem4 && npm run test:problem5",
    "test:problem4": "cd src/problem4 && npm test",
    "test:problem5": "cd src/problem5 && npm test",
    "lint:all": "npm run lint:problem4 && npm run lint:problem5",
    "lint:problem4": "cd src/problem4 && npm run lint",
    "lint:problem5": "cd src/problem5 && npm run lint",
    "format:all": "npm run format:problem4 && npm run format:problem5",
    "format:problem4": "cd src/problem4 && npm run format",
    "format:problem5": "cd src/problem5 && npm run format",
    "start:problem4": "cd src/problem4 && npm start",
    "start:problem5": "cd src/problem5 && npm start",
    "dev:problem4": "cd src/problem4 && npm run dev",
    "dev:problem5": "cd src/problem5 && npm run dev",
    "demo:problem4": "cd src/problem4 && npm run demo",
    "benchmark:problem4": "cd src/problem4 && npm run benchmark",
    "setup:problem5": "cd src/problem5 && npm run setup",
    "clean:all": "npm run clean:problem4 && npm run clean:problem5",
    "clean:problem4": "cd src/problem4 && rimraf dist node_modules",
    "clean:problem5": "cd src/problem5 && rimraf dist node_modules"
  },
  "keywords": [
    "backend",
    "typescript",
    "express",
    "api",
    "crud",
    "algorithms",
    "monorepo"
  ],
  "author": "Tai Pham Anh",
  "license": "MIT",
  "devDependencies": {
    "rimraf": "^5.0.0"
  }
}
